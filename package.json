{
  "name": "@quailjs/quail",
  "description": "Accessibility testing in the browser and on the server.",
  "homepage": "http://quailjs.org",
  "version": "2.2.18",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/quailjs/quail.git"
  },
  "main": "./quail.js",
  "scripts": {
    "jscs": "jscs -c .jscsrc src/**/*.js test/assessmentSpecs/testRunner.js test/assessmentSpecs/*.js",
    "eslint": "eslint -c .eslintrc src/**/*.js test/assessmentSpecs/*.js",
    "build": "quail build",
    "compile": "bower install",
    "preversion": "npm test",
    "version": "npm run build && git add -A dist && git commit -m \"Versioned the npm module.\"",
    "postversion": "git push",
    "test": "bower install && grunt test && npm run jscs && npm run eslint && npm run test-runner",
    "selenium": "start-selenium",
    "test-runner": "node ./test/assessmentSpecs/testRunner.js",
    "test-selenium": "start-selenium && npm run test-runner"
  },
  "dependencies": {
    "chai-spies": "^0.5.1",
    "deepmerge": "^0.2.7",
    "http-server": "0.7.3",
    "jquery": ">1.4.2",
    "node-fs-extra": "^0.8.1",
    "phantomjs": "^1.9.18",
    "q": "^1.1.2",
    "selenium-standalone": "2.43.1-5",
    "stdio": "^0.2.7",
    "webdriverio": "2.3.0"
  },
  "devDependencies": {
    "@quailjs/quail-cli": "0.0.8",
    "@quailjs/quail-core": "0.0.9",
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.1",
    "babel-polyfill": "^6.2.0",
    "babelify": "^6.3.0",
    "bower": "^1.3.12",
    "brfs": "^1.4.1",
    "browserify": "^11.0.1",
    "browserify-shim": "^3.8.11",
    "chai": "^1.9.2",
    "chai-as-promised": "^4.1.1",
    "eslint": "^1.3.1",
    "glob": "^4.2.1",
    "grunt": "~0.4.5",
    "grunt-bower-task": "~0.4.0",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-copy": "^0.6.0",
    "grunt-contrib-jshint": "~0.4.3",
    "grunt-contrib-uglify": "~0.2.0",
    "grunt-contrib-watch": "~0.5.3",
    "grunt-convert": "~0.1.5",
    "grunt-exec": "^0.4.6",
    "grunt-execute": "^0.2.2",
    "grunt-gh-pages": "~0.8.1",
    "grunt-run": "^0.4.0",
    "gulp-gzip": "^1.2.0",
    "gulp-uglify": "^1.5.1",
    "jscs": "jscs-dev/node-jscs#c5adeba",
    "mkdirp": "^0.5.1",
    "mocha": "^2.0.1",
    "should": "^4.3.0",
    "sinon": "^1.10.3"
  },
  "browserify-shim": {
    "external": "global:External"
  }
}
